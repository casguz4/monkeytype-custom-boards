{
    "name": "monkeytype-custom-boards",
    "description": "a website with a form that takes in a list of users and returns a view with a data comparison",
    "type": "module",
    "scripts": {
        "build": "react-router build",
        "cf-typegen": "wrangler types && react-router typegen",
        "check": "pnpm run typecheck && react-router build && wrangler deploy --dry-run",
        "deploy": "wrangler deploy",
        "dev": "react-router dev",
        "format": "oxfmt",
        "format:check": "oxfmt --check",
        "format:fix": "oxmft --fix",
        "gen:readme": "auto-readme -g ./README.md",
        "lint": "oxlint --type-aware --type-check --report-unused-disable-directives --max-warnings 0",
        "lint:fix": "pnpm run lint --fix",
        "prepare": "node .husky/install.mjs",
        "preview": "pnpm run build && vite preview",
        "typecheck": "pnpm run cf-typegen && pnpm run lint"
    },
    "dependencies": {
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-select": "^2.2.5",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-switch": "^1.2.5",
        "@radix-ui/react-tabs": "^1.1.12",
        "@tanstack/react-form": "^1.19.1",
        "@tanstack/react-query": "^5.85.0",
        "@typescript/native-preview": "7.0.0-dev.20260105.1",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "isbot": "5.1.26",
        "lodash": "^4.17.21",
        "lucide-react": "^0.539.0",
        "motion": "^12.23.26",
        "next-themes": "^0.4.6",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-hook-form": "^7.62.0",
        "react-router": "7.6.3",
        "sonner": "^2.0.7",
        "tailwind-merge": "^3.3.1"
    },
    "devDependencies": {
        "@cloudflare/vite-plugin": "1.7.5",
        "@eslint/js": "^9.33.0",
        "@hookform/devtools": "^4.4.0",
        "@react-router/dev": "7.6.3",
        "@stephansama/auto-readme": "^0.2.4",
        "@tailwindcss/vite": "4.1.4",
        "@types/eslint__js": "^8.42.3",
        "@types/lodash": "^4.17.20",
        "@types/node": "24.0.4",
        "@types/react": "^19.0.0",
        "@types/react-dom": "^19.0.0",
        "eslint": "^9.33.0",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.7",
        "oxfmt": "^0.22.0",
        "oxlint": "^1.37.0",
        "oxlint-tsgolint": "^0.10.1",
        "tailwindcss": "4.1.1",
        "tw-animate-css": "^1.3.6",
        "typescript": "5.8.3",
        "typescript-eslint": "^8.39.1",
        "vite": "^6.0.0",
        "vite-tsconfig-paths": "5.1.4",
        "wrangler": "4.21.x"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,mjs,cjs}": "pnpm run lint",
        "*": "oxfmt --no-error-on-unmatched-pattern"
    },
    "cloudflare": {
        "label": "React Router Framework Starter",
